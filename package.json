    "scripts": {
        "prebuild": "rimraf dist",
        "build": "set NODE_OPTIONS=--max_old_space_size=4096 && nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:local": "cross-env NODE_ENV=local nest start --watch --debug",
        "start:dev": "cross-env NODE_ENV=development nest start --watch",
        "start:prod": "cross-env NODE_ENV=production node dist/main",
        "start": "node dist/main",
        "command:delete:user:development": "cross-env NODE_ENV=development npx nestjs-command delete:user",
        "command:update-posts:images:development": "cross-env NODE_ENV=development npx nestjs-command update-posts:images",
        "command:update-posts:images:stage": "cross-env NODE_ENV=stage npx nestjs-command update-posts:images",
        "typeorm:local": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d ./src/DAL/typeorm.config.ts",
        "migration:create:local": "npm run typeorm:local migration:create ./src/DAL/migrations/migrations",
        "migration:generate:local": "npm run typeorm:local migration:generate ./src/DAL/migrations/migrations --pretty",
        "migration:up:local": "npm run typeorm:local migration:run",
        "migration:down:local": "npm run typeorm:local migration:revert",
        "migration:create:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:create ./src/DAL/migrations/migrations --outputJs",
        "migration:generate:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate ./src/DAL/migrations/migrations -d ./src/DAL/typeorm.config.ts --pretty --outputJs",
        "migration:up:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:run -d ./src/DAL/typeorm.config.ts",
        "migration:up:local:development": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/DAL/typeorm.config.ts",
        "migration:down:development": "cross-env NODE_ENV=development ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/DAL/typeorm.config.ts",
        "migration:create:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:create ./src/DAL/migrations/migrations --outputJs",
        "migration:generate:production": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:generate ./src/DAL/migrations/migrations -d ./src/DAL/typeorm.config.ts --pretty --outputJs",
        "migration:up:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:run -d ./src/DAL/typeorm.config.ts",
        "migration:down:production": "cross-env NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:revert -d ./src/DAL/typeorm.config.ts"
    },
